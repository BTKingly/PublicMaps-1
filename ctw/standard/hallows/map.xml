<?xml version="1.0"?>
<map proto="1.4.0">
<name>Hallows</name>
<version>1.1.4</version>
<objective>Capture the other teams wools!</objective>
<gamemode>ctw</gamemode>
<authors>
    <author uuid="be49b1a8-600e-4751-9725-a8aead777363"/> <!-- izjust4fun -->
    <author uuid="297428a6-b482-4558-99f5-b413067ed48d"/> <!-- CrazyClownz -->
</authors>
<teams>
    <team id="blue-team" color="blue" max="16">Blue</team>
    <team id="red-team" color="dark red" max="16">Red</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <clear/>
        <item slot="0" material="iron sword" unbreakable="true"/>
        <item slot="1" material="bow" unbreakable="true">
            <enchantment>arrow infinite</enchantment>
        </item>
        <item slot="2" material="iron pickaxe" unbreakable="true">
            <enchantment>dig speed</enchantment>
        </item>
        <item slot="3" material="iron axe" unbreakable="true">
            <enchantment>dig speed</enchantment>
        </item>
        <item slot="4" material="wood" damage="1" amount="48"/>
        <item slot="5" material="glass" amount="16"/>
        <item slot="6" material="bucket"/>
        <item slot="8" material="golden apple"/>
        <item slot="28" material="arrow"/>
        <chestplate material="leather chestplate" team-color="true" unbreakable="true"/>
        <leggings material="chainmail leggings" unbreakable="true"/>
        <boots material="leather boots" team-color="true" unbreakable="true"/>
        <effect duration="5" amplifier="1">heal</effect>
    </kit>
</kits>
<spawns>
    <spawn team="blue-team" kit="spawn-kit">
        <region yaw="-180">
            <cuboid min="-1277.5,11,-873.5" max="-1281.5,11,-874.5"/>
        </region>
    </spawn>
    <spawn team="red-team" kit="spawn-kit">
        <region yaw="0">
            <cuboid min="-1277.5,11,-1130.5" max="-1281.5,11,-1131.5"/>
        </region>
    </spawn>
    <default>
        <region yaw="90">
            <cuboid min="-1208.5,41,-1000" max="-1210.5,41,-1004.5"/>
        </region>
    </default>
</spawns>
<wools craftable="false">
    <wool team="blue-team" color="cyan" monument="cyan-wool-monument" location="-1274,12,-876"/>
    <wool team="blue-team" color="light_blue" monument="lightblue-wool-monument" location="-1286,12,-876"/>
    <wool team="red-team" color="magenta" monument="magenta-wool-monument" location="-1286,12,-1130"/>
    <wool team="red-team" color="pink" monument="pink-wool-monument" location="-1274,12,-1130"/>
</wools>
<filters>
    <team id="only-blue">blue-team</team>
    <team id="only-red">red-team</team>
    <not id="not-void">
        <void/>
    </not>
    <all id="reds-woolrooms-filter">
        <team>blue-team</team>
        <filter id="woolrooms-filter"/>
    </all>
    <all id="blues-woolrooms-filter">
        <team>red-team</team>
        <filter id="woolrooms-filter"/>
    </all>
    <any id="woolrooms-filter">
        <material>web</material>
        <material>wood:1</material>
        <material>glass</material>
        <all>
            <cause>player</cause>
            <any>
                <material>stationary water</material>
                <material>water</material>
            </any>
        </all>
    </any>
    <all id="only-iron-cause-world">
        <material id="only-iron">iron block</material>
        <cause>world</cause>
    </all>
    <material id="only-air">air</material>
    <deny id="deny-redstone">
        <material>redstone wire</material>
    </deny>
</filters>
<regions>
    <block id="cyan-wool-monument">-1274,12,-876</block>
    <block id="light-blue-wool-monument">-1286,12,-876</block>
    <block id="magenta-wool-monument">-1286,12,-1130</block>
    <block id="pink-wool-monument">-1274,12,-1130</block>
    <union id="spawns">
        <cylinder id="blue-spawn" base="-1279.5,0,-862.5" radius="21" height="oo"/>
        <cylinder id="red-spawn" base="-1279.5,0,-1142.5" radius="21" height="oo"/>
    </union>
    <union id="woolrooms">
        <union id="reds-woolrooms">
            <rectangle min="-1331.5,-890.5" max="-1341.5,-899.5"/> <!-- magenta -->
            <rectangle min="-1217.5,-809.5," max="-1227.5,-899.5"/> <!-- pink -->
        </union>
        <union id="blues-woolrooms">
            <rectangle min="-1217.5,-1114.5," max="-1227.5,-1105.5"/> <!-- cyan -->
            <rectangle min="-1331.5,-1114.5" max="-1341.5,-1105.5"/> <!-- light blue -->
        </union>
    </union>
    <negative id="not-build-area">
        <union id="build-area">
            <rectangle min="-1259.5,-974.5" max="-1299.5,-929.5"/>
            <rectangle min="-1259.5,-1029.5" max="-1299.5,-1075.5"/>
        </union>
    </negative>
    <!-- applicators -->
    <apply enter="only-blue" region="blue-spawn" message="You may not enter the enemy's spawn!"/>
    <apply enter="only-red" region="red-spawn" message="You may not enter the enemy's spawn!"/>
    <apply enter="only-red" region="blues-woolrooms" message="You may not enter your own woolroom!"/>
    <apply enter="only-blue" region="reds-woolrooms" message="You may not enter your own woolroom!"/>
    <apply block-place="only-iron-cause-world" block-break="only-iron" region="spawns" message="You may not edit spawn!"/>
    <apply block="blues-woolrooms-filter" region="blues-woolrooms" message="You may not edit the wool room!"/>
    <apply block="reds-woolrooms-filter" region="reds-woolrooms" message="You may not edit the wool room!"/>
    <apply block="not-void" region="not-build-area" message="You may not edit the void area!"/>
    <apply use="only-red" region="blues-woolrooms" message="You may not enter your own woolroom!"/>
    <apply use="only-blue" region="reds-woolrooms" message="You may not enter your own woolroom!"/>
    <apply block-physics="deny-redstone" region="woolrooms"/>
</regions>
<itemkeep>
    <item>iron sword</item>
    <item>bow</item>
    <item>iron pickaxe</item>
    <item>iron axe</item>
    <item>arrow</item>
    <item>wood:1</item>
    <item>glass</item>
    <item>golden apple</item>
    <item>bucket</item>
    <item>water bucket</item>
</itemkeep>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
    <tool>iron pickaxe</tool>
    <tool>iron axe</tool>
    <tool>arrow</tool>
</toolrepair>
<itemremove>
    <item>leather chestplate</item>
    <item>chainmail leggings</item>
    <item>leather boots</item>
    <item>melon</item>
    <item>seeds</item>
    <item>flint</item>
</itemremove>
<block-drops>
    <rule>
        <filter>
            <material>wood:1</material>
        </filter>
        <drops>
            <item chance="0" material="wood" damage="1"/>
        </drops>
    </rule>
</block-drops>
<item-mods>
    <rule>
        <match>
            <material>iron helmet</material>
            <material>iron chestplate</material>
            <material>iron leggings</material>
            <material>iron boots</material>
            <material>iron sword</material>
            <material>iron pickaxe</material>
            <material>iron axe</material>
            <material>iron spade</material>
            <material>shears</material>
            <material>bow</material>
            <material>diamond chestplate</material>
        </match>
        <modify unbreakable="true"/>
    </rule>
</item-mods>
<renewables>
    <renewable rate="0.5" sound="false" region="spawns" renew-filter="only-iron" replace-filter="only-air" avoid-players="5"/>
</renewables>
<maxbuildheight>26</maxbuildheight>
<hunger>
    <depletion>off</depletion>
</hunger>
<kill-rewards>
    <kill-reward>
        <item material="golden apple"/>
        <item material="wood" damage="1" amount="8"/>
    </kill-reward>
</kill-rewards>
</map>

<map proto="1.4.2">
<name>Race for Victory 4</name>
<version>1.0</version>
<objective>Take the enemy's wool located to either side of the enemy's base and place it in your victory monument.</objective>
<authors>
    <author uuid="ef4ea031-998f-4ec9-b7b6-1bdd428bcef8" contribution="Map design"/> <!-- Plastix -->
    <author uuid="25961a08-c90c-4abd-b136-dad90e89c2eb" contribution="Map design"/> <!-- Anxuiz -->
    <author uuid="3c7db14d-ac4b-4e35-b2c6-3b2237f382be" contribution="Map design and code"/> <!-- MonsieurApple/Apple -->
</authors>
<contributors>
    <contributor uuid="9d4779a8-d35f-45d2-b51c-95de7a77e128" contribution="Map decoration and code changes"/> <!-- zzuf -->
</contributors>
<created>2012-08-12</created>
<teams>
    <team id="blue" color="blue" max="24">Blue</team>
    <team id="red" color="dark red" max="24">Red</team>
</teams>
<spawns>
    <spawn team="blue" kit="spawn-kit" yaw="0">
        <regions>
            <cylinder base="0,11,96" radius="3" height="0"/>
        </regions>
    </spawn>
    <spawn team="red" kit="spawn-kit" yaw="180">
        <regions>
            <cylinder base="0,11,-96" radius="3" height="0"/>
        </regions>
    </spawn>
    <default yaw="90">
        <regions>
            <cylinder base="-50,58,0" radius="3" height="0"/>
        </regions>
    </default>
</spawns>
<kits>
    <kit id="spawn-kit">
        <clear/>
        <item slot="0" unbreakable="true" material="iron sword"/>
        <item slot="1" enchantment="arrow infinite:1">bow</item>
        <item slot="2" enchantment="durability:3;dig speed:1">iron pickaxe</item>
        <item slot="3" enchantment="durability:3;dig speed:1">iron axe</item>
        <item slot="4" amount="64" damage="1" material="wood"/>
        <item slot="5" amount="64" material="glass"/>
        <item slot="6" amount="32" material="ladder"/>
        <item slot="7" material="golden apple"/>
        <item slot="8" amount="64" material="golden carrot"/>
        <item slot="28" material="arrow"/>
        <item slot="31" amount="64" damage="1" material="wood"/>
        <helmet unbreakable="true" team-color="true" material="leather helmet"/>
        <chestplate unbreakable="true" team-color="true" material="leather chestplate"/>
    </kit>
</kits>
<filters>
    <not id="no-void">
        <any>
            <void/>
        </any>
    </not>
    <deny id="no-beacons">
        <material>beacon</material>
    </deny>
    <all id="blue-in-wr">
        <team id="only-blue">blue</team>
        <filter id="wr-filter"/>
    </all>
    <all id="red-in-wr">
        <team id="only-red">red</team>
        <filter id="wr-filter"/>
    </all>
    <any id="wr-filter">
        <material>stained glass pane</material>
        <material>glass</material>
        <material>wood</material>
        <material>ladder</material>
    </any>
    <any id="only-iron">
        <material>iron block</material>
    </any>
    <deny id="deny-players">
          <participating/>
    </deny>
</filters>
<regions>
    <!-- void protection -->
    <negative id="void-area">
        <rectangle id="middle" min="-18,-22" max="18,22"/>
        <region id="red-wool-rooms"/>
        <region id="blue-wool-rooms"/>
        <region id="spawn-protections"/>
        <region id="portal-protections"/>
    </negative>
    <apply use="no-void" block-place="no-void" region="void-area" message="You may not build in the void"/>
    <apply use="no-beacons" block="no-beacons" message="You may not use or break the beacons"/>
    <!-- portal protections -->
    <union id="portal-protections">
        <circle center="12,-75.5" radius="3"/>
        <circle center="-12,-75.5" radius="3"/>
        <circle center="12,75.5" radius="3"/>
        <circle center="-12,75.5" radius="3"/>
    </union>
    <apply block="never" region="portal-protections"/>
    <union id="spawn-protections">
        <rectangle min="-33,82" max="32,109"/>
        <rectangle min="-33,-82" max="32,-109"/>
    </union>
    <apply block-place="deny-players" block-break="only-iron" region="spawn-protections" message="You may only break Iron Blocks in your supply room."/>
    <!-- the wools blue has to get -->
    <complement id="lime-wool">
        <rectangle min="42,-74" max="54,-62"/>
        <rectangle min="53,-74" max="54,-73"/>
        <rectangle min="53,-63" max="54,-62"/>
        <rectangle min="42,-63" max="43,-62"/>
        <rectangle min="42,-74" max="43,-73"/>
    </complement>
    <complement id="pink-wool">
        <rectangle min="-42,-74" max="-54,-62"/>
        <rectangle min="-54,-74" max="-53,-73"/>
        <rectangle min="-53,-63" max="-54,-62"/>
        <rectangle min="-42,-63" max="-43,-62"/>
        <rectangle min="-42,-74" max="-43,-73"/>
    </complement>
    <complement id="yellow-wool">
        <rectangle min="-6,-41" max="6,-29"/>
        <rectangle min="-6,-41" max="-5,-40"/>
        <rectangle min="-6,-30" max="-5,-29"/>
        <rectangle min="6,-30" max="5,-29"/>
        <rectangle min="6,-41" max="5,-40"/>
    </complement>
    <!-- the wools red has to get -->
    <complement id="purple-wool">
        <rectangle min="42,74" max="54,62"/>
        <rectangle min="53,74" max="54,73"/>
        <rectangle min="53,63" max="54,62"/>
        <rectangle min="42,63" max="43,62"/>
        <rectangle min="42,74" max="43,73"/>
    </complement>
    <complement id="orange-wool">
        <rectangle min="-42,74" max="-54,62"/>
        <rectangle min="-54,74" max="-53,73"/>
        <rectangle min="-53,63" max="-54,62"/>
        <rectangle min="-42,63" max="-43,62"/>
        <rectangle min="-42,74" max="-43,73"/>
    </complement>
    <complement id="cyan-wool">
        <rectangle min="-6,41" max="6,29"/>
        <rectangle min="-6,41" max="-5,40"/>
        <rectangle min="-6,30" max="-5,29"/>
        <rectangle min="6,30" max="5,29"/>
        <rectangle min="6,41" max="5,40"/>
    </complement>
    <!-- the wools red has to get -->
    <union id="red-wool-rooms">
        <region id="purple-wool"/>
        <region id="orange-wool"/>
        <region id="cyan-wool"/>
    </union>
    <!-- the wools blue has to get -->
    <union id="blue-wool-rooms">
        <region id="lime-wool"/>
        <region id="pink-wool"/>
        <region id="yellow-wool"/>
    </union>
    <!-- wool room rules -->
    <apply block="blue-in-wr" use="only-blue" region="blue-wool-rooms" message="You may not interact with this block here."/>
    <apply enter="only-blue" region="blue-wool-rooms" message="You may not enter your own wool room."/>
    <apply block="red-in-wr" use="only-red" region="red-wool-rooms" message="You may not interact with this block here."/>
    <apply enter="only-red" region="red-wool-rooms" message="You may not enter your own wool room."/>
</regions>
<portals>
    <!-- portals in red base -->
    <portal x="1" y="-4" z="14">
        <region>
            <cuboid min="9,11,-88" max="12,14,-87"/>
        </region>
    </portal>
    <portal x="-1" y="-4" z="14">
        <region>
             <cuboid min="-9,11,-88" max="-12,14,-87"/>
        </region>
    </portal>
    <!-- portals in blue base -->
    <portal x="1" y="-4" z="-14">
        <region>
            <cuboid min="9,11,87" max="12,14,88"/>
        </region>
    </portal>
    <portal x="-1" y="-4" z="-14">
        <region>
            <cuboid min="-9,11,87" max="-12,14,88"/>
        </region>
    </portal>
    <!-- portal back to blue base -->
    <portal filter="only-blue" x="1" y="4" z="14">
        <region>
            <cuboid min="-11,7,75" max="-13,11,76"/>
        </region>
    </portal>
    <portal filter="only-blue" x="-1" y="4" z="14">
        <region>
            <cuboid min="11,7,75" max="13,11,76"/>
        </region>
    </portal>
    <!-- portal back to red base -->
    <portal filter="only-red" x="-1" y="4" z="-14">
        <region>
            <cuboid min="-11,7,-75" max="-13,11,-76"/>
        </region>
    </portal>
    <portal filter="only-red" x="1" y="4" z="-14">
        <region>
            <cuboid min="11,7,-75" max="13,11,-76"/>
        </region>
    </portal>
</portals>
<toolrepair>
    <tool>iron sword</tool>
    <tool>iron pickaxe</tool>
    <tool>iron axe</tool>
    <tool>bow</tool>
    <tool>arrow</tool>
</toolrepair>
<itemremove>
    <item>golden carrot</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>seeds</item>
    <item>sugar cane</item>
    <item>string</item>
    <item>glowstone dust</item>
</itemremove>
<block-drops>
    <rule>
        <filter>
            <material>wood</material>
        </filter>
        <drops>
            <item chance="0" material="wood"/>
        </drops>
    </rule>
    <rule>
        <filter>
            <material>ladder</material>
        </filter>
        <drops>
            <item chance="0" material="ladder"/>
        </drops>
    </rule>
</block-drops>
<itemkeep>
    <item>golden apple</item>
    <item>wood</item>
    <item>glass</item>
    <item>ladder</item>
</itemkeep>
<kill-reward>
    <item material="golden apple"/>
    <item amount="32" damage="1" material="wood"/>
    <item amount="16" material="glass"/>
</kill-reward>
<maxbuildheight>27</maxbuildheight>
<wools team="blue">
    <wool color="yellow" location="-0.5,20.5,-34.5">
        <monument><block>0,13,105</block></monument>
    </wool>
    <wool color="pink" location="-47.5,15.5,-67.5">
        <monument><block>-1,12,105</block></monument>
    </wool>
    <wool color="lime" location="47.5,15.5,-67.5">
        <monument><block>0,11,105</block></monument>
    </wool>
</wools>
<wools team="red">
    <wool color="cyan" location="-0.5,20.5,34.5">
        <monument><block>-1,13,-106</block></monument>
    </wool>
    <wool color="purple" location="47.5,15.5,67.5">
        <monument><block>0,12,-106</block></monument>
    </wool>
    <wool color="orange" location="-47.5,15.5,67.5">
        <monument><block>-1,11,-106</block></monument>
    </wool>
</wools>
<renewables>
    <renewable region="spawn-protections" renew-filter="only-iron" interval="4" grow="false" particles="true" sound="true"/>
</renewables>
</map>
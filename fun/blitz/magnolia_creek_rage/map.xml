<map proto="1.4.2">
<name>Magnolia Creek</name>
<version>1.0.3</version>
<objective>Elimate the enemy team before time runs out!</objective>
<gamemode>br</gamemode>
<authors>
    <author uuid="a4266139-34f3-46c5-a730-f8828376a15a"/> <!-- LArrowHead -->
</authors>
<teams>
    <team id="Inhabitants" color="yellow" max="20" max-overfill="25">Inhabitants</team>
    <team id="Explorers" color="dark aqua" max="20" max-overfill="25">Explorers</team>
</teams>
<kits>
    <kit id="spawn-kit" force="true">
        <item slot="0" enchantment="damage all:10" material="wood sword"/>
        <item slot="1" enchantment="arrow damage:10" material="bow"/>
        <item slot="2" material="arrow"/>
        <item slot="3" amount="8" material="golden carrot"/>
        <chestplate team-color="true" material="leather chestplate"/>
        <boots team-color="true" material="leather boots"/>
        <effect duration="3" amplifier="25">heal</effect>
        <effect duration="3" amplifier="25">damage resistance</effect>
        <effect>speed</effect>
        <game-mode>adventure</game-mode>
    </kit>
</kits>
<spawns>
    <spawn team="Inhabitants" kit="spawn-kit">
        <regions yaw="90">
            <point>-196.5,36,-90.5</point>
        </regions>
    </spawn>
    <spawn team="Explorers" kit="spawn-kit">
        <regions yaw="-90">
            <point>-336.5,36,-91.5</point>
        </regions>
    </spawn>
    <default>
        <regions yaw="90">
            <point>-265.5,83,-90.5</point>
        </regions>
    </default>
</spawns>
<regions>
    <apply block="never"/>
</regions>
<time>5m</time>
<blitz>
    <lives>2</lives>
</blitz>
<rage/>
<itemremove>
    <item>golden carrot</item>
    <item>wood sword</item>
    <item>arrow</item>
    <item>bow</item>
    <item>leather chestplate</item>
    <item>leather boots</item>
</itemremove>
<kill-rewards>
    <kill-reward>
        <item material="arrow"/>
    </kill-reward>
</kill-rewards>
<disabledamage>
    <damage>fall</damage>
</disabledamage>
<respawn auto="true"/>
</map>

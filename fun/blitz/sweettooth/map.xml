<map proto="1.4.2">
<name>Sweet Tooth</name>
<version>1.0.8</version>
<objective>Elimate the enemy team before time runs out!</objective>
<gamemode>br</gamemode>
<authors>
    <author uuid="38ab65dd-95f9-406f-917c-6accc995421f"/> <!-- GoldenOre -->
    <author uuid="0eadbe6d-0a2e-41c6-aec3-e1ba291fb266"/> <!-- Cheesedaman -->
</authors>
<rules>
    <rule>Fall damage is disabled.</rule>
</rules>
<teams>
    <team id="lime" color="green" max="20" max-overfill="25">Lime</team>
    <team id="blue" color="blue" max="20" max-overfill="25">Blue</team>
</teams>
<spawns>
    <spawn team="blue" kit="spawn-kit">
        <regions yaw="90">
            <point>71.5,52,3.5</point>
        </regions>
    </spawn>
    <spawn team="lime" kit="spawn-kit">
        <regions yaw="-90">
            <point>-69.5,52,-4.5</point>
        </regions>
    </spawn>
    <default>
        <regions yaw="-90">
            <block>1.5,68,0.5</block>
        </regions>
    </default>
</spawns>
<kits>
    <kit id="spawn-kit">
        <item slot="0" enchantment="damage all:10" material="gold sword"/>
        <item slot="1" enchantment="arrow damage:10" material="bow"/>
        <item slot="2" material="arrow"/>
        <item slot="3" amount="14" material="golden carrot"/>
        <helmet material="gold helmet"/>
        <chestplate team-color="true" material="leather chestplate"/>
        <leggings team-color="true" material="leather leggings"/>
        <boots material="gold boots"/>
        <effect duration="5" amplifier="25">heal</effect>
        <effect duration="5" amplifier="25">damage resistance</effect>
        <effect duration="oo" amplifier="2">speed</effect>
        <game-mode>adventure</game-mode>
    </kit>
</kits>
<regions>
    <apply block="never"/>
</regions>
<time>5m</time>
<blitz>
    <lives>1</lives>
    <broadcastLives>true</broadcastLives>
</blitz>
<rage/>
<itemremove>
    <item>golden carrot</item>
    <item>gold sword</item>
    <item>arrow</item>
    <item>bow</item>
    <item>leather chestplate</item>
    <item>gold boots</item>
    <item>gold helmet</item>
    <item>leather leggings</item>
</itemremove>
<kill-rewards>
    <kill-reward>
        <item material="arrow"/>
    </kill-reward>
</kill-rewards>
<disabledamage>
    <damage>fall</damage>
</disabledamage>
<timelock>on</timelock>
<respawn auto="true"/>
</map>
